  "script": "exports.step = function (input, fileInput) {\n  const {\n    mxPart: {\n      area,\n      availableQuantity,\n      barcode,\n      description,\n      extraFields,\n      minimumQuantity,\n      name,\n      partTypes,\n      unitCost,\n      vendors,\n    },\n    plantId,\n    sapId,\n    unitOfMeasureMap,\n    materialTypesMap,\n    defaultMaterialType,\n    defaultUnitOfMeasure,\n    defaultWeightUnit,\n    defaultVolumeUnit,\n  } = input;\n\n  const sapUnitCost = unitCost ? unitCost / 100 : 0;\n  const sapDescription = description || name;\n  const unitOfMeasure =\n    unitOfMeasureMap[extraFields[\"Unit of Measure\"]] ??\n    unitOfMeasureMap[defaultUnitOfMeasure];\n  const weightUnit =\n    unitOfMeasureMap[extraFields[\"Weight Unit\"]] ??\n    unitOfMeasureMap[defaultWeightUnit];\n  const materialType =\n    materialTypesMap[extraFields[\"Material Type\"]] ??\n    materialTypesMap[defaultMaterialType];\n  const volumeUnit =\n    unitOfMeasureMap[extraFields[\"Volume Unit\"]] ??\n    unitOfMeasureMap[defaultVolumeUnit];\n\n  return {\n    Product: sapId,\n    ProductType: materialType,\n    IndustrySector: \"M\",\n    ProductGroup: \"L002\",\n    BaseISOUnit: unitOfMeasure,\n    VolumeISOUnit: volumeUnit,\n    WeightISOUnit: weightUnit,\n    _ProductDescription: [\n      {\n        Product: sapId,\n        Language: \"EN\",\n        ProductDescription: sapDescription,\n      },\n    ],\n    _ProductPlant: [\n      {\n        Plant: plantId,\n        Product: sapId,\n        _ProductPlantSales: {\n          Plant: plantId,\n          Product: sapId,\n          AvailabilityCheckType: \"ST\",\n        },\n      },\n    ],\n  };\n};\n",
  "file_output": false
}
